<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>addServlet</servlet-name>
  	<display-name>addServlet</display-name>
  	<description></description>
  	<servlet-class>biz.zc.servlet.addServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>findServlet</servlet-name>
  	<display-name>findServlet</display-name>
  	<description></description>
  	<servlet-class>biz.zc.servlet.findServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>addServlet</servlet-name>
  	<url-pattern>/addServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>findServlet</servlet-name>
  	<url-pattern>/findServlet</url-pattern>
  </servlet-mapping>
</web-app>
